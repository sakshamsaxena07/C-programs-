#include <stdio.h>

struct student {
    int roll_no;
    char fname[20];
    char lname[20];
};

int main() {
    struct student s[5], temp;
    int i, j;

    for(i = 0; i < 5; i++) {
        printf("\nEnter Roll No: ");
        scanf("%d", &s[i].roll_no);
        printf("Enter First Name: ");
        scanf("%s", s[i].fname);
        printf("Enter Last Name: ");
        scanf("%s", s[i].lname);
    }

    // Sorting by roll number
    for(i = 0; i < 5; i++) {
        for(j = i + 1; j < 5; j++) {
            if(s[i].roll_no > s[j].roll_no) {
                temp = s[i];
                s[i] = s[j];
                s[j] = temp;
            }
        }
    }

    printf("\n--- SORTED STUDENTS ---\n");
    for(i = 0; i < 5; i++) {
        printf("\nRoll No: %d | Name: %s %s", s[i].roll_no, s[i].fname, s[i].lname);
    }

    return 0;
}
